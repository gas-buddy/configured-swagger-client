{
  "name": "@gasbuddy/configured-swagger-client",
  "version": "3.2.2",
  "description": "A module that creates a set of swagger clients with support for inter-service tracing, custom certificates, and separated endpoint configuration",
  "main": "build/index.js",
  "scripts": {
    "test": "tap --node-arg=-r --node-arg=babel-register test/*.js",
    "cover": "cross-env BABEL_ENV=test nyc tap test/test_*.js && nyc check-coverage && nyc report --reporter=text",
    "ci-test": "cross-env NODE_ENV=test nyc tap -Rtap test/test_*.js | tee .tap_output | tap -Rspec -",
    "ci-cover": "nyc check-coverage",
    "ci-coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint .",
    "build": "babel src --out-dir build",
    "prepublish": "npm run build"
  },
  "keywords": [
    "gasbuddy",
    "swagger",
    "client",
    "services"
  ],
  "author": "GasBuddy",
  "license": "MIT",
  "engines": {
    "node": "^8"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.16.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-gasbuddy": "^3.3.3",
    "babel-register": "^6.16.3",
    "cross-env": "^5.1.4",
    "eslint-config-gasbuddy": "^4.1.0",
    "nock": "^9.2.3",
    "nyc": "^11.6.0",
    "tap": "^11.1.3"
  },
  "dependencies": {
    "@gasbuddy/swagger-ref-resolver": "^4.2.0",
    "lodash": "^4.17.5",
    "swagger-client": "^3.5.2",
    "winston": "^2.4.1"
  },
  "nyc": {
    "lines": 75,
    "statements": 75,
    "functions": 75,
    "branches": 45,
    "include": [
      "src"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "reporter": [
      "text",
      "lcov"
    ]
  }
}
