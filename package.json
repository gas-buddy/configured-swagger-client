{
  "name": "@gasbuddy/configured-swagger-client",
  "version": "1.3.2",
  "description": "A module that creates a set of swagger clients with support for inter-service tracing, custom certificates, and separated endpoint configuration",
  "main": "build/index.js",
  "scripts": {
    "test": "tap --node-arg=-r --node-arg=babel-register test/*.js",
    "cover": "nyc --reporter=lcov npm test",
    "lint": "eslint .",
    "build": "babel src --out-dir build",
    "prepublish": "npm run build"
  },
  "keywords": [
    "gasbuddy",
    "swagger",
    "client",
    "services"
  ],
  "author": "GasBuddy",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-preset-gasbuddy": "^1",
    "babel-register": "^6.16.3",
    "eslint-config-gasbuddy": "^1",
    "nock": "^8.0.0",
    "nyc": "^8.3.0",
    "tap": "^7.1.2"
  },
  "dependencies": {
    "@gasbuddy/swagger-ref-resolver": "^1.0.1",
    "lodash": "^4.16.4",
    "swagger-client": "~2.1.23",
    "winston": "^2.2.0"
  }
}
