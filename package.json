{
  "name": "@gasbuddy/configured-swagger-client",
  "version": "5.4.2",
  "description": "A module that creates a set of swagger clients with support for inter-service tracing, custom certificates, and separated endpoint configuration",
  "main": "build/index.js",
  "scripts": {
    "test": "tap --node-arg=-r --node-arg=@babel/register --no-coverage test/*.js",
    "ci-test": "tap test/*.js",
    "lint": "eslint .",
    "build": "babel src --out-dir build",
    "prepublish": "npm run build"
  },
  "keywords": [
    "gasbuddy",
    "swagger",
    "client",
    "services"
  ],
  "author": "GasBuddy",
  "license": "MIT",
  "engines": {
    "node": "^8"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/register": "^7.9.0",
    "babel-preset-gasbuddy": "^6.0.0",
    "cross-env": "^7.0.2",
    "eslint-config-gasbuddy": "^5.3.0",
    "nock": "^12.0.3",
    "rest-api-support": "^1.6.1",
    "tap": "^14.10.7"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "eventsource": "^1.0.7",
    "form-data": "^3.0.0",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0"
  },
  "nyc": {
    "lines": 75,
    "statements": 75,
    "functions": 75,
    "branches": 45,
    "include": [
      "src"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": true,
    "reporter": [
      "text",
      "lcov"
    ]
  }
}
